/usr/bin/moc-qt4:
	(cd "$(QTDIR)/src/tools/moc" && $(MAKE))

mocclean: compiler_moc_header_clean compiler_moc_source_clean

mocables: compiler_moc_header_make_all compiler_moc_source_make_all

compiler_moc_header_make_all: moc_EditorPane.cpp moc_Gui.cpp moc_SyntaxHighlighter.cpp moc_TextEditor.cpp
compiler_moc_header_clean:
	-$(DEL_FILE) moc_EditorPane.cpp moc_Gui.cpp moc_SyntaxHighlighter.cpp moc_TextEditor.cpp
moc_EditorPane.cpp: TextEditor.H \
		EditorPane.H \
		/usr/bin/moc-qt4
	/usr/bin/moc-qt4 $(DEFINES) $(INCPATH) EditorPane.H -o moc_EditorPane.cpp

moc_Gui.cpp: Gui.H \
		/usr/bin/moc-qt4
	/usr/bin/moc-qt4 $(DEFINES) $(INCPATH) Gui.H -o moc_Gui.cpp

