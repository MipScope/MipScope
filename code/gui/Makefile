		/usr/bin/moc-qt4
	/usr/bin/moc-qt4 $(DEFINES) $(INCPATH) SyntaxHighlighter.H -o moc_SyntaxHighlighter.cpp

moc_TextEditor.cpp: TextEditor.H \
